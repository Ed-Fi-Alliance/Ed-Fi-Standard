					<xs:documentation>Points awarded to a student in recognition of his or her achievements.</xs:documentation>
					<xs:appinfo>
						<ann:EdFiId>120-015</ann:EdFiId>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Prize awards">
				<xs:annotation>
					<xs:documentation>An award given to a candidate for one time or ongoing performance.</xs:documentation>
					<xs:appinfo>
						<ann:EdFiId>120-016</ann:EdFiId>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Promotion or advancement">
				<xs:annotation>
					<xs:documentation>Promotion or advancement awarded to a student in recognition of his/her achievements (e.g., advancement from regular to honors class, and promotion from junior varsity to varsity).</xs:documentation>
					<xs:appinfo>
						<ann:EdFiId>120-017</ann:EdFiId>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Scholarships">
				<xs:annotation>
					<xs:documentation>An award of financial aid for a student to further his or her education.</xs:documentation>
					<xs:appinfo>
						<ann:EdFiId>120-018</ann:EdFiId>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
