			<xs:element name="MaximumScore" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The maximum score to make the indicated level of performance.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
