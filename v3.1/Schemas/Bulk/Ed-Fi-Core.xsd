          <xs:documentation>The maximum score possible on the assessment.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ResultDatatypeType" type="ResultDatatypeTypeDescriptorReferenceType" minOccurs="0">
